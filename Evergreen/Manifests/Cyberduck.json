{
	"Name": "Cyberduck",
	"Source": "https://cyberduck.io/",
	"Get": {
		"Uri": {
			"Release" : "https://version.cyberduck.io/windows/changelog.rss"
		},
		"ContentType": "text/xml",
		"EnclosureXmlNode": "//enclosure",
		"ItemXmlNode": "//item",
		"DatePattern": "dd/MM/yyyy hh:mm tt"
	},
	"Install": {
		"Setup": "Cyberduck*.exe",
		"Physical": {
			"Arguments": "/install /quiet /norestart",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/install /quiet /norestart",
			"PostInstall": []
		}
	}
}
