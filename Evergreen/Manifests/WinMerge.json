{
	"Name": "WinMerge",
	"Source": "https://winmerge.org/",
	"Get": {
		"Uri": "https://sourceforge.net/projects/winmerge/best_release.json",
		"MatchVersion": "(\\d+(\\.\\d+){1,3})",
		"DatePattern": "yyyy-MM-dd HH:mm:ss",
		"DownloadUri": "https://nchc.dl.sourceforge.net/project/winmerge/stable/#Version/#Filename"
	},
	"Install": {
		"Setup": "AppName.*.exe",
		"Physical": {
			"Arguments": "",
			"PostInstall": [
			]
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": [
				"Get-Service -Name AdobeARMservice -ErrorAction SilentlyContinue | Set-Service -StartupType Disabled"
			]
		}
	}
}
