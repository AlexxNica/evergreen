{
	"Name": "OpenJDK",
	"Source": "https://openjdk.java.net/",
	"Get": {
		"Uri": "https://api.github.com/repos/ojdkbuild/ojdkbuild/releases",
		"MatchVersion": ".*[-](\\d+(\\.\\d+){1,4}.*$)",
		"MatchFileTypes": "\\.exe$|\\.msi$|\\.msp$"
	},
	"Install": {
		"Setup": "java*.msi",
		"Physical": {
			"Arguments": "",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "",
			"PostInstall": []
		}
	}
}
