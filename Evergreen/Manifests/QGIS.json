{
	"Name": "QGIS",
	"Source": "https://qgis.org/",
	"Get": {
		"Update":{
			"Uri": "https://raw.githubusercontent.com/qgis/QGIS-Website/master/source/schedule.py",
			"MatchStringData": ".*=.*"
		},
		"Download":{
			"Uri": "https://qgis.org/downloads/QGIS-OSGeo4W-#version-#msibinary.msi",
			"ReplaceVersion": "#version",
			"ReplaceMsiBinary": "#msibinary"

		},
		"Releases":{
			"LTR":{
				"Edition": "ltrrelease",
				"MSIBinary": "msiltrbinary"
			},
			"Latest":{
				"Edition": "release",
				"MSIBinary": "msibinary"
			}
		}
	},
	"Install": {
		"Setup": "QGIS-OSGeo4W-*.msi",
		"Physical": {
			"Arguments": "/install /passive /norestart",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/install /passive /norestart",
			"PostInstall": []
		}
	}
}