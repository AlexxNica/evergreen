{
	"Name": "mRemoteNG",
	"Source": "https://mremoteng.org/",
	"Get": {
		"Uri": "https://api.github.com/repos/mRemoteNG/mRemoteNG/releases/latest",
		"ContentType": "application/json; charset=utf-8",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*"
	},
	"Install": {
		"Setup": "mRemoteNG*.msi",
		"Physical": {
			"Arguments": "/quiet /noreboot",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/quiet /noreboot",
			"PostInstall": []
		}
	}
}
